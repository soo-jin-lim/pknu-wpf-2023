<Window x:Class="wp05_bikeshop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wp05_bikeshop"
        mc:Ignorable="d"
        Title="오토바이" Height="450" Width="670" MaxWidth="760" MaxHeight="480" FontFamily="NanumGothic">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="42*"/>
            <ColumnDefinition Width="293*"/>
        </Grid.ColumnDefinitions>
        <!-- 자식페이지를 할당할 수 있는-->
        <Frame x:Name="MainFrame" Background="Gainsboro"
               NavigationUIVisibility="Visible" Source="/MenuPage.xaml" Grid.ColumnSpan="2" />
    </Grid>
</Window>
